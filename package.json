{
  "name": "gitbook-plugin-theme-api",
  "description": "Theme for using GitBook as an API documentation",
  "version": "1.0.2",
  "engines": {
    "gitbook": ">=3.0.1"
  },
  "dependencies": {
    "cheerio": "0.20.0",
    "gitbook-plugin-search": ">=2.0.0",
    "lodash": "4.12.0",
    "q": "1.4.1",
    "q-plus": "0.0.8"
  },
  "devDependencies": {
    "eslint": "2.9.0",
    "less": "2.6.0",
    "less-plugin-clean-css": "^1.5.1",
    "preboot": "git+https://github.com/mdo/preboot.git#4aab4edd85f076d50609cbe28e4fe66cc0771701",
    "uglify-js": "2.6.1"
  },
  "scripts": {
    "prepublish": "./src/build.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GitbookIO/theme-api.git"
  },
  "author": "FriendCode Inc. <contact@gitbook.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/GitbookIO/theme-api/issues"
  },
  "contributors": [
    {
      "name": "Johan Preynat",
      "email": "johan@gitbook.com"
    }
  ],
  "gitbook": {
    "properties": {
      "languages": {
        "type": "array",
        "description": "List of languages code snippets",
        "default": []
      },
      "split": {
        "type": "boolean",
        "description": "Initial layout for this documentation",
        "default": true
      },
      "theme": {
        "type": "string",
        "default": "light",
        "title": "Default theme",
        "enum": [
          "light",
          "dark"
        ]
      }
    }
  }
}
